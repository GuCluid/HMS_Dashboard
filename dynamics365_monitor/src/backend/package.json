{
  "name": "dynamics365-monitor",
  "version": "1.0.0",
  "description": "Monitoring dashboard for Dynamics 365",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "cd ../frontend && npm run build",
    "install-all": "npm install && cd ../frontend && npm install --legacy-peer-deps",
    "dev": "nodemon server.js",
    "deploy": "npm run build && npm start"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Dynamics 365 Monitor Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
